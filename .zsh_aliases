# .zsh_aliases
#
# Shell aliases.

alias e='emacs -nw'
alias g='git'
alias gch='git checkout'
alias gcm='git commit'
alias gb='git branch'
alias gl='git log'
alias gdf='git diff'

alias ls='ls --color=auto'
alias grep='grep --color=auto'
alias lynx='lynx -nopause'

alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'

alias bm='set_bookmark'
alias b='get_bookmark'
alias to='goto_bookmark'
